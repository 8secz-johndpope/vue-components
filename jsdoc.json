{
  "tags": {
      "allowUnknownTags": false
  },
  "source": {
      "include": "./src",
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs|assets)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts": {
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true
  },
  "template": "node_modules/docdash",
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  }
}
